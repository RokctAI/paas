{
    "doctype": "DocType",
    "name": "Ads Package",
    "engine": "InnoDB",
    "is_submittable": 0,
    "issingle": 0,
    "istable": 0,
    "module": "paas",
    "permissions": [
        {
            "create": 1,
            "delete": 1,
            "email": 1,
            "export": 1,
            "print": 1,
            "read": 1,
            "report": 1,
            "role": "System Manager",
            "share": 1,
            "write": 1
        },
        {
            "role": "Seller",
            "read": 1
        }
    ],
    "sort_field": "modified",
    "sort_order": "DESC",
    "track_changes": 1,
    "fields": [
        {
            "fieldname": "title",
            "fieldtype": "Data",
            "label": "Title",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "price",
            "fieldtype": "Currency",
            "label": "Price",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "time_type",
            "fieldtype": "Select",
            "label": "Time Type",
            "options": "minute\nhour\nday\nweek\nmonth\nyear",
            "default": "day",
            "reqd": 1
        },
        {
            "fieldname": "time",
            "fieldtype": "Int",
            "label": "Time (Duration)",
            "reqd": 1,
            "default": 1
        },
        {
            "fieldname": "active",
            "fieldtype": "Check",
            "label": "Active",
            "default": "1"
        },
        {
            "fieldname": "translations",
            "fieldtype": "Table",
            "label": "Translations",
            "options": "Ads Package Translation"
        }
    ]
}