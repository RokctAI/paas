{
    "doctype": "DocType",
    "name": "Product",
    "engine": "InnoDB",
    "is_submittable": 0,
    "issingle": 0,
    "istable": 0,
    "module": "paas",
    "permissions": [
        {
            "create": 1,
            "delete": 1,
            "email": 1,
            "export": 1,
            "print": 1,
            "read": 1,
            "report": 1,
            "role": "System Manager",
            "share": 1,
            "write": 1
        }
    ],
    "sort_field": "modified",
    "sort_order": "DESC",
    "track_changes": 1,
    "fields": [
        {
            "fieldname": "title",
            "fieldtype": "Data",
            "label": "Title",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "shop",
            "fieldtype": "Link",
            "label": "Shop",
            "options": "Shop",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "category",
            "fieldtype": "Link",
            "label": "Category",
            "options": "Category"
        },
        {
            "fieldname": "brand",
            "fieldtype": "Link",
            "label": "Brand",
            "options": "Brand"
        },
        {
            "fieldname": "unit",
            "fieldtype": "Link",
            "label": "Unit",
            "options": "Shop Unit"
        },
        {
            "fieldname": "price",
            "fieldtype": "Currency",
            "label": "Price",
            "in_list_view": 1
        },
        {
            "fieldname": "cost",
            "fieldtype": "Currency",
            "label": "Cost"
        },
        {
            "fieldname": "image",
            "fieldtype": "Attach Image",
            "label": "Image"
        },
        {
            "fieldname": "status",
            "fieldtype": "Select",
            "label": "Status",
            "options": "pending\npublished\nunpublished",
            "default": "published",
            "in_list_view": 1
        },
        {
            "fieldname": "active",
            "fieldtype": "Check",
            "label": "Active",
            "default": "1"
        },
        {
            "fieldname": "description",
            "fieldtype": "Text Editor",
            "label": "Description"
        },
        {
            "fieldname": "vegetarian",
            "fieldtype": "Check",
            "label": "Vegetarian",
            "default": "0"
        },
        {
            "fieldname": "addon",
            "fieldtype": "Check",
            "label": "Addon",
            "default": "0"
        },
        {
            "fieldname": "min_qty",
            "fieldtype": "Int",
            "label": "Min Quantity",
            "default": 1
        },
        {
            "fieldname": "track_stock",
            "fieldtype": "Check",
            "label": "Track Stock",
            "default": "1"
        },
        {
            "fieldname": "max_qty",
            "fieldtype": "Int",
            "label": "Max Quantity"
        },
        {
            "fieldname": "calories",
            "fieldtype": "Float",
            "label": "Calories (kcal)"
        },
        {
            "fieldname": "carbs",
            "fieldtype": "Float",
            "label": "Carbs (g)"
        },
        {
            "fieldname": "protein",
            "fieldtype": "Float",
            "label": "Protein (g)"
        },
        {
            "fieldname": "fats",
            "fieldtype": "Float",
            "label": "Fats (g)"
        },
        {
            "fieldname": "translations",
            "fieldtype": "Table",
            "label": "Translations",
            "options": "Product Translation"
        },
        {
            "fieldname": "extras",
            "fieldtype": "Table",
            "label": "Extras",
            "options": "Product Extra Group Link"
        }
    ]
}