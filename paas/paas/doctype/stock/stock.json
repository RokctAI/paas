            "in_list_view": 1
        },
        {
            "fieldname": "quantity",
            "fieldtype": "Int",
            "label": "Quantity",
            "default": "0",
            "in_list_view": 1
        },
        {
            "fieldname": "image",
            "fieldtype": "Attach Image",
            "label": "Image"
        },
        {
            "fieldname": "addon",
            "fieldtype": "Check",
            "label": "Is Addon",
            "default": "0"
        },
        {
            "fieldname": "stock_extras",
            "fieldtype": "Table",
            "label": "Extras",
            "options": "Stock Extra Link"
        }
    ]
}