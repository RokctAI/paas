{
    "doctype": "DocType",
    "name": "Delivery Zone",
    "engine": "InnoDB",
    "is_submittable": 0,
    "issingle": 0,
    "istable": 0,
    "module": "paas",
    "permissions": [
        {
            "create": 1,
            "delete": 1,
            "email": 1,
            "export": 1,
            "print": 1,
            "read": 1,
            "report": 1,
            "role": "System Manager",
            "share": 1,
            "write": 1
        },
        {
            "role": "Seller",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 1
        }
    ],
    "sort_field": "modified",
    "sort_order": "DESC",
